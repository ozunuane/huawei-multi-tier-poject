######## generatetoken #########

curl -X -k POST https://10.83.1.40:10012/ajipayaccounts/generatetoken  \
     -H "Content-Type: application/json" \
     -H "HashValue: FlyingSaucer=2024" \
     -H "Sender: 190511" \
     -d '{
           "username": "999928",
           "password": "Flying$_03_22_Higher"
         }'

         
